{
  "name": "KlubitusReact",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build": "babel src/ -d build/",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "watch": "yarn run build -- --watch",
    "web-bundle": "node node_modules/webpack/bin/webpack --config web/webpack/web.prod.config.js --progress --colors",
    "web-dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server --content-base web/public/ --config web/webpack/web.dev.config.js --port 3001 --inline --hot --colors"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "graphql-tag": "^2.2.0",
    "react": "16.0.0-alpha.6",
    "react-apollo": "^1.2.0",
    "react-dom": "^15.6.1",
    "react-native": "0.44.0",
    "react-native-vector-icons": "^4.1.1",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "1.9.2",
    "jest": "20.0.3",
    "react-test-renderer": "16.0.0-alpha.6",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
