    "react-apollo": "^1.2.0",
    "react-dom": "16.0.0-alpha.12",
    "react-native": "0.48.1",
    "react-native-vector-icons": "^4.3.0",
    "react-navigation": "v1.0.0-beta.12",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
